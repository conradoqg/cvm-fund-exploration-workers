    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch All Workers",
            "program": "${workspaceFolder}/bin/cvmfe.js",
            "env": {
                "EOD_TOKEN": ""
            },
            "args": [
                "run",
                "all",
                "sampleData",
                "interestingFundsOnly"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
